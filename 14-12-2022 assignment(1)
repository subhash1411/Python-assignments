n=int(input("Enter size of list:"))
print("Enter Elements into LIST:")
s=[]
for i in range(0,n):
    str=int(input())
    s.append(str)

max=min=s[0]
for i in range(1,n):
    if max<s[i]:
        max=s[i]
    if min>s[i]:
        min=s[i]
print("Maxmimum value is :",max)
print("Minimum value is :",min)
